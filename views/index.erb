<a href="?filter=uniq">Show uniq</a>
<p></p>
<table>
	<tr>
		<th>Case Number</th>
		<th>Message</th>
		<th>Occured at</th>
	</tr>
	<% @errors.each do |error| %>
		<tr>
			<td><%= error.case_number %></td>
			<td><i><%= error.message %></i></td>
			<td><%= error.occured_at %></td>
		</tr>
	<% end %>
</table>